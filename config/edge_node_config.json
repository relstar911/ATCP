{
    "edge_nodes": {
        "default_config": {
            "cpu_cores": 4,
            "ram_gb": 16,
            "storage_gb": 256,
            "network_bandwidth_mbps": 100
        },
        "logging": {
            "level": "INFO",
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
            "file": "logs/edge_node.log"
        },
        "preprocessing": {
            "queue_size": 1000,
            "batch_size": 32,
            "timeout_seconds": 30
        }
    }
} 